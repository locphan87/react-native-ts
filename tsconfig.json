{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": false,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "jsx": "react-native",
    "typeRoots": [
      "node_modules/@types",
      "./typings/index.d.ts"
    ],
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "strictFunctionTypes": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "build",
    "sourceMap": true,
    "strict": true,
    "target": "es2017"
  },
  "types": [
    "react",
    "react-dom",
    "react-native"
  ],
  "exclude":[
    "build",
    "./src/__mocks__",
    "node_modules",
    "jest.config.js",
    "App.js"
  ]
}
