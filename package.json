{
  "name": "react-native-ts",
  "version": "0.1.5",
  "private": true,
  "devDependencies": {
    "@types/enzyme": "3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/enzyme-to-json": "^1.5.2",
    "@types/graphql": "^14.0.0",
    "@types/jest": "23.3.2",
    "@types/prettier": "^1.13.2",
    "@types/react": "16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-native": "0.56.18",
    "@types/react-native-i18n": "^2.0.0",
