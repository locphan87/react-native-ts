    "@types/recompose": "^0.26.4",
    "@types/redux-actions": "^2.3.0",
    "@types/redux-promise-middleware": "^0.0.10",
    "@types/seamless-immutable": "^7.1.2",
    "babel-preset-react-native": "4.0.1",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "enzyme-to-json": "3.3.4",
    "husky": "0.14.3",
    "jest-expo": "^30.0.0",
    "lint-staged": "7.2.2",
    "prettier": "1.14.2",
    "react-dom": "16.3.1",
    "react-native-scripts": "1.14.0",
    "react-native-typescript-transformer": "1.2.10",
    "react-test-renderer": "16.3.1",
    "ts-jest": "23.10.0-beta.6",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.11.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "typescript": "3.0.3",
    "validate-commit": "3.4.0"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "commitmsg": "validate-commit-msg",
    "server": "json-server --watch ./data.json",
    "precommit": "lint-staged",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "setup": "make setup",
    "start": "make start",
    "typecheck": "tsc --project tsconfig.json --noEmit --skipLibCheck",
    "lint": "tslint --project tsconfig.json --config tslint.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "eject": "react-native-scripts eject",
    "android": "make run-android",
    "ios": "make run-ios",
    "deploy": "make deploy-expo",
    "build:android": "make build-android",
    "build:ios": "make build-ios",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "test:wc": "yarn test:watch --coverage",
    "test:ci": "yarn test:coverage --bail"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "yarn lint:fix",
      "git add"
    ]
  },
  "dependencies": {
    "@types/expo": "27.0.9",
    "@types/ramda": "0.25.38",
    "apollo-boost": "0.1.16",
    "axios": "0.18.0",
    "babel-plugin-dotenv-import": "2.0.0",
    "expo": "^30.0.0",
    "formik": "1.3.0",
    "glamorous-native": "1.4.0",
    "graphql": "14.0.2",
    "key-mirror-nested": "1.3.0",
    "ramda": "^0.25.0",
    "ramda-adjunct": "2.10.0",
    "react": "16.3.1",
    "react-apollo": "2.1.11",
    "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz",
    "react-native-i18n": "2.0.15",
    "react-navigation": "2.14.2",
    "react-redux": "^5.0.6",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-persist": "5.10.0",
    "redux-promise-middleware": "5.1.1",
    "redux-thunk": "^2.3.0",
    "reselect": "3.0.1",
    "rnts-ui": "https://github.com/phanhoangloc/rnts-ui.git#v1.0.6",
    "seamless-immutable": "7.1.4",
    "type-to-reducer": "1.1.0"
  }
